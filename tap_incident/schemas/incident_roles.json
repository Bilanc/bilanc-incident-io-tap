{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": ["string", "null"]
    },
    "instructions": {
      "type": ["string", "null"]
    },
    "required": {
      "type": "boolean"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "time_extracted": {
      "type": ["string"],
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": ["id", "name", "required", "created_at", "updated_at"]
}